    {
      "cell_type": "code",
      "source": [
        "# 1. Suddivisione del dataset con 70% train, 15% val, 15% test\n",
        "unique_ids = df['id'].unique()\n",
        "shuffled_ids = pd.Series(unique_ids).sample(frac=1, random_state=42).values\n",
        "\n",
        "# Dividiamo in train (70%) e temp (30%)\n",
        "train_ids, temp_ids = train_test_split(shuffled_ids, test_size=0.3, random_state=42)\n",
        "\n",
        "# Dividiamo temp in val (15%) e test (15%)\n",
        "val_ids, test_ids = train_test_split(temp_ids, test_size=0.5, random_state=42)\n",
        "\n",
        "# Creiamo i DataFrame per ogni set\n",
        "train_df = df[df['id'].isin(train_ids)]\n",
        "val_df = df[df['id'].isin(val_ids)]\n",
        "test_df = df[df['id'].isin(test_ids)]\n",
        "\n",
        "# Controlliamo le dimensioni\n",
        "print(\"Dimensioni dei set:\")\n",
        "print(\"Train set:\", train_df.shape)\n",
        "print(\"Validation set:\", val_df.shape)\n",
        "print(\"Test set:\", test_df.shape)\n"
      ],
      "metadata": {
        "colab": {
