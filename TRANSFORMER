      "cell_type": "markdown",
      "source": [
        "##rebalncing training set"
      ],
      "metadata": {
        "id": "ZvwRxy8OI2Ux"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "# Bilancia solo il training set\n",
        "balanced_train_df = pd.DataFrame()\n",
        "for label in train_df['Label'].unique():\n",
        "    subset = train_df[train_df['Label'] == label]\n",
        "    if len(subset) < train_df['Label'].value_counts().max():\n",
        "        subset = resample(subset, replace=True, n_samples=train_df['Label'].value_counts().max(), random_state=42)\n",
        "    balanced_train_df = pd.concat([balanced_train_df, subset])\n",
        "\n",
        "# Shuffle per mescolare i dati bilanciati\n",
        "balanced_train_df = balanced_train_df.sample(frac=1, random_state=42).reset_index(drop=True)\n",
        "\n",
        "print(\"\\nDistribuzione delle etichette nel training set bilanciato:\")\n",
        "print(balanced_train_df['Label'].value_counts())"
